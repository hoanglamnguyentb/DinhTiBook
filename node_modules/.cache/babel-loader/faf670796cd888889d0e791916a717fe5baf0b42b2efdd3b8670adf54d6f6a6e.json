{"ast":null,"code":"import APIService from \"@/helpers/ApiService\";\nexport default {\n  data() {\n    return {\n      open: false,\n      labelCol: {\n        span: 6\n      },\n      wrapperCol: {\n        span: 14\n      },\n      id: undefined,\n      nxb: {\n        maNXB: '',\n        tenNXB: ''\n      },\n      rules: {\n        maNXB: {\n          required: true,\n          message: 'Mã không được để trống',\n          trigger: 'change'\n        },\n        tenNXB: {\n          required: true,\n          message: 'Tên không được để trống',\n          trigger: 'change'\n        }\n      }\n    };\n  },\n  methods: {\n    async showModal(id) {\n      this.open = true;\n      if (id) {\n        var res = await APIService.get(`/NhaXuatBan/${id}`);\n        this.id = id;\n        this.nxb = res.data.data;\n      }\n    },\n    closeModal() {\n      this.open = false;\n      this.id = undefined;\n      this.resetForm();\n    },\n    handleOk() {\n      this.$refs.formRef.validate().then(async () => {\n        if (this.id) {\n          await APIService.put(`/NhaXuatBan/edit/${this.id}`, this.chuyenmuc);\n          this.$message.success('Thêm thông tin thành công');\n        } else {\n          await APIService.post(\"NhaXuatBan/Create\", this.nxb);\n          this.$message.success('Thêm thành công');\n        }\n        this.closeModal();\n        this.$emit('addSuccess');\n      }).catch(error => {\n        console.log('error', error);\n      });\n    },\n    resetForm() {\n      this.$refs.formRef.resetFields();\n    }\n  }\n};","map":{"version":3,"names":["APIService","data","open","labelCol","span","wrapperCol","id","undefined","nxb","maNXB","tenNXB","rules","required","message","trigger","methods","showModal","res","get","closeModal","resetForm","handleOk","$refs","formRef","validate","then","put","chuyenmuc","$message","success","post","$emit","catch","error","console","log","resetFields"],"sources":["D:\\DA\\DoAnClient\\client\\src\\views\\dashboard\\QL_NXB\\ModalAddEditNXB.vue"],"sourcesContent":["<template>\r\n  <div>\r\n    <a-modal\r\n      v-model:open=\"open\"\r\n      :title=\"id ? 'Cập nhật nhà xuất bản' : 'Thêm nhà xuất bản'\"\r\n      @ok=\"handleOk\"\r\n      @cancel=\"closeModal\"\r\n    >\r\n      <a-form ref=\"formRef\" :model=\"nxb\"  layout=\"vertical\"  :rules=\"rules\">\r\n        <a-form-item ref=\"maNXB\" label=\"Mã NXB\" name=\"maNXB\">\r\n          <a-input v-model:value=\"nxb.maNXB\" />\r\n        </a-form-item>\r\n        <a-form-item ref=\"tenNXB\" label=\"Tên NXB\" name=\"tenNXB\">\r\n          <a-input v-model:value=\"nxb.tenNXB\" />\r\n        </a-form-item>\r\n       \r\n        \r\n        \r\n      </a-form>\r\n    </a-modal>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport APIService from \"@/helpers/ApiService\"\r\nexport default {\r\n    data() {\r\n    return {\r\n      open: false,\r\n      labelCol: {\r\n        span: 6,\r\n      },\r\n      wrapperCol: {\r\n        span: 14,\r\n      },\r\n      id: undefined,\r\n      nxb: {\r\n        maNXB: '',\r\n        tenNXB:'',     \r\n      },\r\n      rules:{\r\n        maNXB:{\r\n          required: true, message: 'Mã không được để trống', trigger: 'change'\r\n        },\r\n        tenNXB:{\r\n          required: true, message: 'Tên không được để trống', trigger: 'change'\r\n        },\r\n       \r\n        \r\n      },\r\n      \r\n    };\r\n  },\r\n  methods:{\r\n    async showModal(id) {\r\n      this.open = true\r\n      if(id) {\r\n        var res = await APIService.get(`/NhaXuatBan/${id}`)\r\n        this.id = id\r\n        this.nxb = res.data.data\r\n      }\r\n    },\r\n    closeModal() {\r\n      this.open = false\r\n      this.id = undefined\r\n      this.resetForm()\r\n    },\r\n    handleOk() {\r\n      this.$refs.formRef.validate().then(async () => {\r\n        \r\n        if (this.id) {\r\n          await APIService.put(`/NhaXuatBan/edit/${this.id}`,this.chuyenmuc) \r\n          this.$message.success('Thêm thông tin thành công')\r\n        }\r\n        else {\r\n          await APIService.post(\"NhaXuatBan/Create\",this.nxb)\r\n          this.$message.success('Thêm thành công')\r\n        }\r\n        this.closeModal()\r\n        this.$emit('addSuccess')\r\n      }).catch(error => {\r\n        console.log('error', error);\r\n      });\r\n    },\r\n    resetForm() {\r\n      this.$refs.formRef.resetFields();\r\n    },\r\n  }\r\n}\r\n</script>\r\n\r\n<style>\r\n\r\n</style>"],"mappings":"AAwBA,OAAOA,UAAS,MAAO,sBAAqB;AAC5C,eAAe;EACXC,IAAIA,CAAA,EAAG;IACP,OAAO;MACLC,IAAI,EAAE,KAAK;MACXC,QAAQ,EAAE;QACRC,IAAI,EAAE;MACR,CAAC;MACDC,UAAU,EAAE;QACVD,IAAI,EAAE;MACR,CAAC;MACDE,EAAE,EAAEC,SAAS;MACbC,GAAG,EAAE;QACHC,KAAK,EAAE,EAAE;QACTC,MAAM,EAAC;MACT,CAAC;MACDC,KAAK,EAAC;QACJF,KAAK,EAAC;UACJG,QAAQ,EAAE,IAAI;UAAEC,OAAO,EAAE,wBAAwB;UAAEC,OAAO,EAAE;QAC9D,CAAC;QACDJ,MAAM,EAAC;UACLE,QAAQ,EAAE,IAAI;UAAEC,OAAO,EAAE,yBAAyB;UAAEC,OAAO,EAAE;QAC/D;MAGF;IAEF,CAAC;EACH,CAAC;EACDC,OAAO,EAAC;IACN,MAAMC,SAASA,CAACV,EAAE,EAAE;MAClB,IAAI,CAACJ,IAAG,GAAI,IAAG;MACf,IAAGI,EAAE,EAAE;QACL,IAAIW,GAAE,GAAI,MAAMjB,UAAU,CAACkB,GAAG,CAAE,eAAcZ,EAAG,EAAC;QAClD,IAAI,CAACA,EAAC,GAAIA,EAAC;QACX,IAAI,CAACE,GAAE,GAAIS,GAAG,CAAChB,IAAI,CAACA,IAAG;MACzB;IACF,CAAC;IACDkB,UAAUA,CAAA,EAAG;MACX,IAAI,CAACjB,IAAG,GAAI,KAAI;MAChB,IAAI,CAACI,EAAC,GAAIC,SAAQ;MAClB,IAAI,CAACa,SAAS,CAAC;IACjB,CAAC;IACDC,QAAQA,CAAA,EAAG;MACT,IAAI,CAACC,KAAK,CAACC,OAAO,CAACC,QAAQ,CAAC,CAAC,CAACC,IAAI,CAAC,YAAY;QAE7C,IAAI,IAAI,CAACnB,EAAE,EAAE;UACX,MAAMN,UAAU,CAAC0B,GAAG,CAAE,oBAAmB,IAAI,CAACpB,EAAG,EAAC,EAAC,IAAI,CAACqB,SAAS;UACjE,IAAI,CAACC,QAAQ,CAACC,OAAO,CAAC,2BAA2B;QACnD,OACK;UACH,MAAM7B,UAAU,CAAC8B,IAAI,CAAC,mBAAmB,EAAC,IAAI,CAACtB,GAAG;UAClD,IAAI,CAACoB,QAAQ,CAACC,OAAO,CAAC,iBAAiB;QACzC;QACA,IAAI,CAACV,UAAU,CAAC;QAChB,IAAI,CAACY,KAAK,CAAC,YAAY;MACzB,CAAC,CAAC,CAACC,KAAK,CAACC,KAAI,IAAK;QAChBC,OAAO,CAACC,GAAG,CAAC,OAAO,EAAEF,KAAK,CAAC;MAC7B,CAAC,CAAC;IACJ,CAAC;IACDb,SAASA,CAAA,EAAG;MACV,IAAI,CAACE,KAAK,CAACC,OAAO,CAACa,WAAW,CAAC,CAAC;IAClC;EACF;AACF","