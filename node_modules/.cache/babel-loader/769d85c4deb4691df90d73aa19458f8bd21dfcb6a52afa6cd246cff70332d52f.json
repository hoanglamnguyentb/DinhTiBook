{\r\n        callback();\r\n      }\r\n    },\r\n    handleOk() {\r\n      this.$refs.formRef.validate().then(async () => {\r\n        \r\n        if (this.id) {\r\n          await APIService.put(`/ThongTinDanhMuc/edit/${this.id}`,this.chuyenmuc) \r\n          this.$message.success('Cập nhật huyện thành công')\r\n        }\r\n        else {\r\n           await APIService.post(\"ThongTinDanhMuc/create\",this.chuyenmuc)\r\n           console.log('chhuydsen muc', this.chuyenmuc)\r\n           this.$message.success('Thêm chuyên mục thành công')\r\n        }\r\n        this.closeModal()\r\n        this.$emit('addSuccess')\r\n      }).catch(error => {\r\n        console.log('error', error);\r\n      });\r\n    },\r\n    resetForm() {\r\n      this.$refs.formRef.resetFields();\r\n    },\r\n  }\r\n};\r\n</script>\r\n\r\n<style>\r\n</style>"],"mappings":"AA8BA,OAAOA,UAAS,MAAO,sBAAsB;AAC7C,SAASC,OAAM,QAAS,uBAAuB;AAC/C,eAAe;EACbC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,IAAI,EAAE,KAAK;MACXC,QAAQ,EAAE;QACRC,IAAI,EAAE;MACR,CAAC;MACDC,UAAU,EAAE;QACVD,IAAI,EAAE;MACR,CAAC;MACDE,EAAE,EAAEC,SAAS;MACbC,SAAS,EAAE;QACTC,MAAM,EAAE,EAAE;QACVC,UAAU,EAAC,EAAE;QACbC,YAAY,EAAE,EAAE;QAChBC,MAAM,EAAE,EAAE;QACVC,SAAS,EAAC;MACZ,CAAC;MACDC,KAAK,EAAC;QACJJ,UAAU,EAAC;UACTK,QAAQ,EAAE,IAAI;UAAEC,OAAO,EAAE,mCAAmC;UAAEC,OAAO,EAAE;QACzE,CAAC;QACDR,MAAM,EAAC;UACLM,QAAQ,EAAE,IAAI;UAAEC,OAAO,EAAE,oCAAoC;UAAEC,OAAO,EAAE;QAC1E,CAAC;QACDN,YAAY,EAAC,CACX;UACEI,QAAQ,EAAE,IAAI;UAAEC,OAAO,EAAE,4BAA4B;UAAEC,OAAO,EAAE;QAClE,CAAC,EACD;UACEC,IAAI,EAAE,SAAS;UAAEF,OAAO,EAAE,wBAAwB;UAAEC,OAAO,EAAE;QAC/D,CAAC,EACD;UACEE,SAAS,EAAEC,YAAY;UACvBH,OAAO,EAAE;QACX;MAGJ;IAEF,CAAC;EACH,CAAC;EACDI,OAAO,EAAC;IACN,MAAMC,SAASA,CAAChB,EAAE,EAAE;MAClB,IAAI,CAACJ,IAAG,GAAI,IAAG;MACf,IAAGI,EAAE,EAAE;QACL,IAAIiB,GAAE,GAAI,MAAMxB,UAAU,CAACyB,GAAG,CAAE,oBAAmBlB,EAAG,EAAC;QACvD,IAAI,CAACA,EAAC,GAAIA,EAAC;QACX,IAAI,CAACE,SAAQ,GAAIe,GAAG,CAACtB,IAAI,CAACA,IAAG;MAC/B;IACF,CAAC;IACDwB,UAAUA,CAAA,EAAG;MACX,IAAI,CAACvB,IAAG,GAAI,KAAI;MAChB,IAAI,CAACI,EAAC,G