{"ast":null,"code":"import APIService from \"@/helpers/ApiService\";\nimport { integer } from '@vuelidate/validators';\nexport default {\n  data() {\n    return {\n      open: false,\n      labelCol: {\n        span: 6\n      },\n      wrapperCol: {\n        span: 14\n      },\n      id: undefined,\n      chuyenmuc: {\n        giaTri: '',\n        maThongTin: '',\n        thuTuHienThi: '',\n        ghiChu: '',\n        maDanhMuc: '03'\n      },\n      rules: {\n        maThongTin: {\n          required: true,\n          message: 'Mã chuyên mục không được để trống',\n          trigger: 'change'\n        },\n        giaTri: {\n          required: true,\n          message: 'Tên chuyên mục không được để trống',\n          trigger: 'change'\n        },\n        thuTuHienThi: [{\n          required: true,\n          message: 'Thứ tự không được để trống',\n          trigger: 'change'\n        }, {\n          type: 'integer',\n          message: 'Thứ tự phải là kiểu số',\n          trigger: 'blur'\n        }, {\n          validator: trungIdOrTen,\n          trigger: 'change'\n        }]\n      }\n    };\n  },\n  methods: {\n    async showModal(id) {\n      this.open = true;\n      if (id) {\n        var res = await APIService.get(`/ThongTinDanhMuc/${id}`);\n        this.id = id;\n        this.chuyenmuc = res.data.data;\n      }\n    },\n    closeModal() {\n      this.open = false;\n      this.id = undefined;\n      this.resetForm();\n    },\n    trungIdOrTen(value, callback) {\n      const duplicate = this.chuyenmuc.filter(item => item.maThongTin == value);\n      if (duplicate.length > 1) {\n        callback(new Error('Mã bị trùng lặp'));\n      } else {\n        callback();\n      }\n    },\n    handleOk() {\n      this.$refs.formRef.validate().then(async () => {\n        if (this.id) {\n          await APIService.put(`/ThongTinDanhMuc/edit/${this.id}`, this.chuyenmuc);\n          this.$message.success('Cập nhật huyện thành công');\n        } else {\n          await APIService.post(\"ThongTinDanhMuc/create\", this.chuyenmuc);\n          console.log('chhuydsen muc', this.chuyenmuc);\n          this.$message.success('Thêm chuyên mục thành công');\n        }\n        this.closeModal();\n        this.$emit('addSuccess');\n      }).catch(error => {\n        console.log('error', error);\n      });\n    },\n    resetForm() {\n      this.$refs.formRef.resetFields();\n    }\n  }\n};","map":{"version":3,"names":["APIService","integer","data","open","labelCol","span","wrapperCol","id","undefined","chuyenmuc","giaTri","maThongTin","thuTuHienThi","ghiChu","maDanhMuc","rules","required","message","trigger","type","validator","trungIdOrTen","methods","showModal","res","get","closeModal","resetForm","value","callback","duplicate","filter","item","length","Error","handleOk","$refs","formRef","validate","then","put","$message","success","post","console","log","$emit","catch","error","resetFields"],"sources":["D:\\HINET_1\\BacKan\\client\\src\\views\\dashboard\\QL_ChuyenMucTinTuc\\ModalAddEditChuyenMucTT.vue"],"sourcesContent":["<template>\r\n  <div>\r\n    <a-modal\r\n      v-model:open=\"open\"\r\n      :title=\"id ? 'Cập nhật chuyên mục' : 'Thêm chuyên mục'\"\r\n      @ok=\"handleOk\"\r\n      @cancel=\"closeModal\"\r\n    >\r\n      <a-form ref=\"formRef\" :model=\"chuyenmuc\"  layout=\"vertical\"  :rules=\"rules\">\r\n        <a-form-item ref=\"giaTri\" label=\"Mã chuyên mục\" name=\"maThongTin\">\r\n          <a-input v-model:value=\"chuyenmuc.maThongTin\" />\r\n        </a-form-item>\r\n        <a-form-item ref=\"giaTri\" label=\"Tên chuyên mục\" name=\"giaTri\">\r\n          <a-input v-model:value=\"chuyenmuc.giaTri\" />\r\n        </a-form-item>\r\n        <a-form-item ref=\"thuTuHienThi\" label=\"Thứ tự hiển thị\" name=\"thuTuHienThi\">\r\n          <a-input v-model:value=\"chuyenmuc.thuTuHienThi\" />\r\n        </a-form-item>\r\n        <a-form-item ref=\"ghiChu\" label=\"Ghi chú\" name=\"ghiChu\">\r\n          <!-- <a-input v-model:value=\