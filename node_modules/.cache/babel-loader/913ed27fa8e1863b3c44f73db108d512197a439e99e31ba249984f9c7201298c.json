 Xử lý lỗi cụ thể\n          if (errors.noiBat) {\n            toast.error(errors.noiBat[0], {\n              theme: \"colored\",\n              autoClose: 2000\n            });\n          }\n          if (errors.tintuc) {\n            toast.error(errors.tintuc[0], {\n              theme: \"colored\",\n              autoClose: 2000\n            });\n          }\n        } else {\n          console.error('Lỗi không xác định:', e);\n          toast.error(\"Có lỗi xảy ra. Vui lòng thử lại sau.\", {\n            theme: \"colored\",\n            autoClose: 2000\n          });\n        }\n      }\n    },\n    async getNXB() {\n      var result = await APIService.get(`/NhaXuatBan/`);\n      this.nxb = result.data.data.items;\n      for (var i = 0; i < this.nxb.length; i++) {\n        var nxbItem = {\n          value: this.nxb[i].maNXB,\n          label: this.nxb[i].tenNXB\n        };\n        this.lstNXB.push(nxbItem);\n      }\n    }\n  },\n  watch: {\n    newProductId: function (newVal, oldVal) {\n      if (newVal) {\n        // Gọi hàm uploadFiles của FileManager khi newProductId thay đổi\n        this.$refs.childRef.handleUpload(newVal);\n        console.log('đã ');\n      }\n    }\n  },\n  created() {\n    this.getNXB();\n  }\n};","map":{"version":3,"names":["FileManager","appConfig","APIService","PageHeader","toast","components","data","page","title","meta","name","content","description","datetimeConfig","dateFormat","sanpham","tenSach","tenTacGia","idNhaXuatBan","namXuatBan","idDanhMuc","giaTien","soLuongTon","moTaSach","idNhomDoTuoi","noiBat","giamGia","sachKhuyenDoc","newProductId","lstNXB","nxb","showFileManager","rules","TieuDe","required","message","trigger","methods","onSubmit","post","then","response","console","log","id","success","theme","autoClose","e","submitted","isLoading","status","errors","error","tintuc","getNXB","result","get","items","i","length","nxbItem","value","maNXB","label","tenNXB","push","watch","newVal","oldVal","$refs","childRef","handleU