lstNXB.push(nxbItem);\r\n      }\r\n    }\r\n  },\r\n  watch:{\r\n    newProductId: function(newVal, oldVal) {\r\n      if (newVal) {\r\n        // Gọi hàm uploadFiles của FileManager khi newProductId thay đổi\r\n        this.$refs.childRef.handleUpload(newVal);\r\n        console.log('đã ')\r\n      }\r\n    }\r\n  },\r\n  created(){\r\n    this.getNXB();\r\n  }\r\n  \r\n\r\n}\r\n</script>\r\n\r\n<style>\r\n\r\n</style>"],"mappings":";AAgHA,OAAOA,WAAU,MAAO,iDAAiD;AACzE,OAAOC,SAAQ,MAAO,wBAAwB;AAC9C,OAAOC,UAAS,MAAO,sBAAsB;AAC7C,OAAOC,UAAS,MAAO,0BAA0B;AAEjD,SAASC,KAAI,QAAS,eAAe;AACrC,OAAO,8BAA8B;AACrC,eAAe;EACbC,UAAU,EAAC;IACTF,UAAU;IACVH;EAEF,CAAC;EACDM,IAAIA,CAAA,EAAE;IACJ,OAAM;MACJC,IAAI,EAAE;QACJC,KAAK,EAAE,mBAAmB;QAC1BC,IAAI,EAAE,CACJ;UACEC,IAAI,EAAE,mBAAmB;UACzBC,OAAO,EAAEV,SAAS,CAACW;QACrB,CAAC;MAEL,CAAC;MACDC,cAAc,EAAE;QACd;QACAC,UAAU,EAAE;QACZ;MACF,CAAC;MACDC,OAAO,EAAC;QACNC,OAAO,EAAE,EAAE;QACXC,SAAS,EAAE,EAAE;QACbC,YAAY,EAAE,EAAE;QAChBC,UAAU,EAAE,EAAE;QACdC,SAAS,EAAE,EAAE;QACbC,OAAO,EAAE,EAAE;QACXC,UAAU,EAAE,EAAE;QACdC,QAAQ,EAAE,EAAE;QACZC,YAAY,EAAE,EAAE;QAChBC,MAAM,EAAE,KAAK;QACbC,OAAO,EAAE,EAAE;QACXC,aAAa,EAAE;MACjB,CAAC;MACDC,YAAY,EAAE,EAAE;MAChBC,MAAM,EAAE,EAAE;MACVC,GAAG,EAAC,EAAE;MACNC,eAAe,EAAE,KAAK;MACtBC,KAAK,EAAC;QACLC,MAAM,EAAC,CACN;UACEC,QAAQ,EAAE,IAAI;UACdC,OAAO,EAAE,4BAA4B;UACrCC,OAAO,EAAE;QACX;MAEF;IACF;EACF,CAAC;EACDC,OAAO,EAAC;IACN,MAAMC,QAAQA,CAAA,EAAG;MACf,IAAG;QACDpC,UAAU,CAACqC,IAAI,CAAC,gBAAgB,EAAE,IAAI,CAACxB,OAAO,EAC7CyB,IAAI,CAACC,QAAO,IAAI;UACfC,OAAO,CAACC,GAAG,CAACF,QAAQ,CAACnC,IAAI,CAACA,IAAI,CAACsC,EAAE;UACjC,IAAI,CAAChB,YAAW,GAAIa,QAAQ,CAACnC,IAAI,CAACA,IAAI,CAACsC,EAAC;UACxCxC,KAAK,CAACyC,OAAO,CAAC,0BAA0B,EAAE;YACxCC,KAAK,EAAE,SAAS;YAChBC,SAAS,EAAE;UACb,CAAC,CAAC;QACJ,CAAC;MAGH,CAAC,QAAMC,CAAC,EAAC;QACP,IAAI,CAACC,SAAQ,GAAI,KAAK;QACtB,IAAI,CAACC,SAAQ,GAAI,KAAK;QACtB,IAAIF,CAAC,CAACP,QAAO,IAAKO,CAAC,CAACP,QAAQ,CAACU,MAAK,KAAM,GAAE,IAAKH,CAAC,CAACP,QAAQ,CAACnC,IAAI,CAAC8C,MAAM,EAAE;UACrE,MAAM;YAAEA;UAAO,IAAIJ,CAAC,CAACP,QAAQ,CAAC