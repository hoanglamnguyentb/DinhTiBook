o`);\r\n      this.user = res.data.data;\r\n      this.dataSource[0].value = this.user.fullName;\r\n      this.dataSource[1].value = this.user.email;\r\n      this.dataSource[2].value = this.user.emailComfirmed\r\n        ? \"Đã xác thực\"\r\n        : \"Chưa xác thực\";\r\n      this.dataSource[3].value = this.user.phoneNumber;\r\n      this.dataSource[4].value = this.user.phoneNumberComfirmed\r\n        ? \"Đã xác thực\"\r\n        : \"Chưa xác thực\";\r\n      this.dataSource[5].value = this.user.twoFactorEnable ? \"Bật\" : \"Tắt\";\r\n      this.dataSource[5].color = this.user.twoFactorEnable ? \"green\" : \"red\";\r\n      //       this.dataSource[5].value = `<a-tag :color=\"this.user.twoFactorEnable ? 'blue' : 'gray'\" @click=\"toggleTwoFactor\">\r\n      //   {this.user.twoFactorEnable ? 'Bật' : 'Tắt'}\r\n      // </a-tag>`;\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\nh3 {\r\n  color: white;\r\n  white-space: nowrap;\r\n}\r\n\r\n.edit-button {\r\n  margin-left: auto;\r\n  margin-top: 50px;\r\n}\r\n.img-background {\r\n  display: flex;\r\n  justify-content: space-between;\r\n  z-index: 1000;\r\n  flex-wrap: wrap;\r\n  display: block;\r\n  color: white;\r\n}\r\n</style>\r\n"],"mappings":"AAiDA,OAAOA,UAAS,MAAO,sBAAsB;AAC7C,OAAOC,mBAAkB,MAAO,2BAA2B;AAC3D,OAAOC,gBAAe,MAAO,wBAAwB;AACrD,eAAe;EACbC,OAAOA,CAAA,EAAG,CAAC,CAAC;EACZC,OAAOA,CAAA,EAAG;IACR,IAAI,CAACC,SAAS,CAAC,CAAC;EAClB,CAAC;EACDC,UAAU,EAAE;IACVL,mBAAmB;IACnBC;EACF,CAAC;EACDK,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,IAAI,EAAE,CAAC,CAAC;MACRC,OAAO,EAAE,CACP;QACEC,SAAS,EAAE;MACb,CAAC,EACD;QACEA,SAAS,EAAE;MACb,CAAC,CACF;MACDC,UAAU,EAAE,CACV;QACEC,GAAG,EAAE,GAAG;QACRC,KAAK,EAAE,WAAW;QAClBC,KAAK,EAAE;MACT,CAAC,EACD;QACEF,GAAG,EAAE,GAAG;QACRC,KAAK,EAAE,eAAe;QACtBC,KAAK,EAAE;MACT,CAAC,EACD;QACEF,GAAG,EAAE,GAAG;QACRC,KAAK,EAAE,gBAAgB;QACvBC,KAAK,EAAE;MACT,CAAC,EACD;QACEF,GAAG,EAAE,GAAG;QACRC,KAAK,EAAE,eAAe;QACtBC,KAAK,EAAE;MACT,CAAC,EACD;QACEF,GAAG,EAAE,GAAG;QACRC,KAAK,EAAE,wBAAwB;QAC/BC,KAAK,EAAE;MACT,C