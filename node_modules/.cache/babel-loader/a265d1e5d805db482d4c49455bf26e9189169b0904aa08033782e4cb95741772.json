{"ast":null,"code":"import { ref, watch } from 'vue';\nimport { useFmgStore } from '@/stores/fmg';\nimport APIService from '@/helpers/ApiService';\nimport { message } from 'ant-design-vue';\nimport { InboxOutlined } from '@ant-design/icons-vue';\nexport default {\n  name: \"FileManager\",\n  components: {\n    InboxOutlined\n  },\n  props: {\n    active: {\n      type: Boolean,\n      default: false\n    },\n    showButton: {\n      type: Boolean,\n      default: false\n    },\n    showPreview: {\n      type: Boolean,\n      default: false\n    },\n    closable: {\n      type: Boolean,\n      default: false\n    },\n    accept: {\n      type: Array,\n      default: []\n    }\n  },\n  setup(props) {\n    const localActive = ref(props.active);\n    const fmgStore = useFmgStore();\n    const layoutFmg = ref(fmgStore.getLayoutType);\n    const styleLayoutFmg = ref(fmgStore.getStyleType);\n    watch(() => fmgStore.layoutType, newValue => {\n      layoutFmg.value = newValue;\n      if (newValue == \"grid\") {\n        styleLayoutFmg.value = \"hinet-files-view-grid\";\n      } else if (newValue == \"group\") {\n        styleLayoutFmg.value = \"hinet-files-view-group\";\n      } else if (newValue == \"list\") {\n        styleLayoutFmg.value = \"hinet-files-view-list\";\n      }\n    }, {\n      deep: true\n    });\n    watch(() => props.active, () => {\n      localActive.value = props.active;\n    });\n    const toggleDialog = () => {\n      localActive.value = !localActive.value;\n    };\n    const hideDialog = () => {\n      localActive.value = false;\n    };\n    const handleChangeLayout = layoutType => {\n      fmgStore.layoutType = layoutType;\n      if (layoutType == \"grid\") {\n        fmgStore.styleType = \"hinet-files-view-grid\";\n      } else if (layoutType == \"group\") {\n        fmgStore.styleType = \"hinet-files-view-group\";\n      } else if (layoutType == \"list\") {\n        fmgStore.styleType = \"hinet-files-view-list\";\n      }\n    };\n    const visibleDetail = ref(false);\n    const showModalDetail = () => {\n      visibleDetail.value = true;\n    };\n    const handleOk = e => {\n      console.log(e);\n      visibleDetail.value = false;\n    };\n    const visibleUpload = ref(false);\n    const showModalUpload = () => {\n      visibleUpload.value = true;\n    };\n\n    //call API\n    const GetAllFileByUser = async () => {\n      try {\n        var res = await APIService.get(\"/FileAndFolder/get\");\n      } catch (error) {}\n    };\n\n    //handle upload\n    const fileList = ref([]);\n    const uploading = ref(false);\n    const handleChange = () => {\n      const formData = new FormData();\n      fileList.value.forEach(file => {\n        formData.append('files[]', file);\n      });\n      uploading.value = true;\n      // const status = info.file.status;\n      // if (status !== 'uploading') {\n      //   console.log(info.file, info.fileList);\n      // }\n      // if (status === 'done') {\n      //   message.success(`${info.file.name} file uploaded successfully.`);\n      // } else if (status === 'error') {\n      //   message.error(`${info.file.name} file upload failed.`);\n      // }\n    };\n    const handleDrop = e => {\n      console.log(e);\n    };\n    const beforeUpload = file => {\n      const isAccept = props.accept.includes(file.type);\n      if (!isAccept) {\n        file.status = 'error';\n        file.errorMessage = 'Định dạng của tập tin không được hỗ trợ.';\n        message.error('Định dạng của tập tin không được hỗ trợ.');\n      }\n      const isLt25M = file.size / 1024 / 1024 < 25;\n      if (!isLt25M) {\n        file.status = 'error';\n        file.errorMessage = 'Tập tin có dung lượng lớn hơn 25MB.';\n        message.error('Tập tin có dung lượng lớn hơn 25MB');\n      }\n      return isAccept && isLt25M;\n    };\n    const handleUpload = async e => {\n      try {\n        //var resUpload = await APIService.post();\n      } catch (error) {}\n    };\n    const handleCloseUpload = () => {\n      visibleUpload.value = false;\n    };\n    return {\n      active: localActive,\n      toggleDialog,\n      hideDialog,\n      layoutFmg,\n      handleChangeLayout,\n      styleLayoutFmg,\n      showModalDetail,\n      handleOk,\n      visibleDetail,\n      GetAllFileByUser,\n      visibleUpload,\n      showModalUpload,\n      handleChange,\n      fileList,\n      uploading,\n      handleDrop,\n      handleUpload,\n      beforeUpload,\n      handleCloseUpload\n    };\n  },\n  onMounted() {\n    this.GetAllFileByUser();\n  }\n};","map":{"version":3,"names":["ref","watch","useFmgStore","APIService","message","InboxOutlined","name","components","props","active","type","Boolean","default","showButton","showPreview","closable","accept","Array","setup","localActive","fmgStore","layoutFmg","getLayoutType","styleLayoutFmg","getStyleType","layoutType","newValue","value","deep","toggleDialog","hideDialog","handleChangeLayout","styleType","visibleDetail","showModalDetail","handleOk","e","console","log","visibleUpload","showModalUpload","GetAllFileByUser","res","get","error","fileList","uploading","handleChange","formData","FormData","forEach","file","append","handleDrop","beforeUpload","isAccept","includes","status","errorMessage","isLt25M","size","handleUpload","handleCloseUpload","onMounted"],"sources":["D:\\HINET_1\\BacKan\\client\\src\\components\\FileManager\\index.vue"],"sourcesContent":["<template>\r\n  <div v-if=\"showButton\" class=\"btn btn-success\" @click=\"toggleDialog\">Chọn tập tin</div>\r\n  <div v-if=\"showPreview\" class=\"item-preview\">\r\n    Đây là Preview\r\n  </div>\r\n\r\n  <a-modal\r\n  v-model:open=\"active\" \r\n  :zIndex=\"9999\"\r\n  width=\"90%\"\r\n  cancel-text=\"Đóng\"\r\n  :closable=\"closable\">\r\n    <template #title>\r\n      <div>\r\n        <a-button key=\"back\">Return</a-button>\r\n        <a-button key=\"submit\" type=\"primary\">Submit</a-button>\r\n      </div>\r\n    </template>\r\n\r\n    <template #footer>\r\n      <a-button v-if=\"closable\" key=\"back\" @click=\"hideDialog\">Đóng</a-button>\r\n    </template>\r\n\r\n    <div class=\"dialog-body\">\r\n      <div class=\"hinet-fmg-body\">\r\n        <div class=\"hinet-fmg-body-head d-none d-lg-flex\">\r\n          <div class=\"hinet-fmg-search\">\r\n            <em class=\"bx bx-search\"></em>\r\n            <input type=\"text\" class=\"form-control border-transparent form-focus-none\"\r\n              placeholder=\"Tìm kiếm tập tin, thư mục\">\r\n          </div>\r\n          <div class=\"hinet-fmg-actions\">\r\n            <ul class=\"hinet-block-tools g-3\">\r\n              <li>\r\n                <a-button type=\"default\" class=\"ant-btn-info\">\r\n                  <template #icon><i class=\"bx bx-folder-plus\"></i></template>\r\n                  Tạo thư mục\r\n                </a-button>\r\n              </li>\r\n              <li>\r\n                <a-button type=\"primary\" @click=\"showModalUpload\">\r\n                  <template #icon><i class=\"bx bx-cloud-upload\"></i></template>\r\n                  Tải lên\r\n                </a-button>\r\n              </li>\r\n            </ul>\r\n          </div>\r\n        </div>\r\n        <div class=\"hinet-fmg-body-content\">\r\n          <div class=\"hinet-block-head hinet-block-head-sm\">\r\n            <div class=\"hinet-block-between position-relative\">\r\n              <div class=\"hinet-block-head-content\">\r\n                <h3 class=\"hinet-block-title page-title\">Quản lý tập tin, thư mục</h3>\r\n              </div>\r\n              <div class=\"hinet-block-head-content\">\r\n                <ul class=\"hinet-block-tools g-1\">\r\n                  <li class=\"d-lg-none\">\r\n                    <a href=\"#\" class=\"btn btn-trigger btn-icon search-toggle toggle-search\" data-target=\"search\">\r\n                      <em class=\"icon ni ni-search\"></em>\r\n                    </a>\r\n                  </li>\r\n                  <li class=\"d-lg-none\">\r\n    