d\r\n        this.NhomDoTuoi = res.data.data\r\n      }\r\n    },\r\n    closeModal() {\r\n      this.open = false\r\n      this.id = undefined\r\n      this.resetForm()\r\n    },\r\n    handleOk() {\r\n      this.$refs.formRef.validate().then(async () => {\r\n        \r\n        if (this.id) {\r\n          await APIService.put(`/NhomDoTuoi/edit/${this.id}`,this.NhomDoTuoi) \r\n          this.$message.success('Cập nhật thông tin thành công')\r\n        }\r\n        else {\r\n          await APIService.post(\"NhomDoTuoi/Create\",this.NhomDoTuoi)\r\n          this.$message.success('Thêm thành công')\r\n        }\r\n        this.closeModal()\r\n        this.$emit('addSuccess')\r\n      }).catch(error => {\r\n        console.log('error', error);\r\n      });\r\n    },\r\n    resetForm() {\r\n      this.$refs.formRef.resetFields();\r\n    },\r\n  }\r\n}\r\n</script>\r\n\r\n<style>\r\n\r\n</style>"],"mappings":"AA0BA,OAAOA,UAAS,MAAO,sBAAqB;AAC5C,eAAe;EACXC,IAAIA,CAAA,EAAG;IACP,OAAO;MACLC,IAAI,EAAE,KAAK;MACXC,QAAQ,EAAE;QACRC,IAAI,EAAE;MACR,CAAC;MACDC,UAAU,EAAE;QACVD,IAAI,EAAE;MACR,CAAC;MACDE,EAAE,EAAEC,SAAS;MACbC,UAAU,EAAE;QACVC,YAAY,EAAE,EAAE;QAChBC,OAAO,EAAC,EAAE;QACVC,UAAU,EAAE;MACd,CAAC;MACDC,KAAK,EAAC;QACJH,YAAY,EAAC;UACXI,QAAQ,EAAE,IAAI;UAAEC,OAAO,EAAE,mCAAmC;UAAEC,OAAO,EAAE;QACzE,CAAC;QACDL,OAAO,EAAC;UACNG,QAAQ,EAAE,IAAI;UAAEC,OAAO,EAAE,mCAAmC;UAAEC,OAAO,EAAE;QACzE;MAGF;IAEF,CAAC;EACH,CAAC;EACDC,OAAO,EAAC;IACN,MAAMC,SAASA,CAACX,EAAE,EAAE;MAClB,IAAI,CAACJ,IAAG,GAAI,IAAG;MACf,IAAGI,EAAE,EAAE;QACL,IAAIY,GAAE,GAAI,MAAMlB,UAAU,CAACmB,GAAG,CAAE,eAAcb,EAAG,EAAC;QAClD,IAAI,CAACA,EAAC,GAAIA,EAAC;QACX,IAAI,CAACE,UAAS,GAAIU,GAAG,CAACjB,IAAI,CAACA,IAAG;MAChC;IACF,CAAC;IACDmB,UAAUA,CAAA,EAAG;MACX,IAAI,CAAClB,IAAG,GAAI,KAAI;MAChB,IAAI,CAACI,EAAC,GAAIC,SAAQ;MAClB,IAAI,CAACc,SAAS,CAAC;IACjB,CAAC;IACDC,QAAQA,CAAA,EAAG;MACT,IAAI,CAACC,KAAK,CAACC,OAAO,CAACC,QAAQ,CAAC,CAAC,CAACC,IAAI,CAAC,YAAY;QAE7C,IAAI,IAAI,CAACpB,EAAE,EAAE;UACX,MAAMN,UAAU,CAAC2B,GAAG,CAAE,oBAAmB,IAAI,CAACrB,EAAG,EAA