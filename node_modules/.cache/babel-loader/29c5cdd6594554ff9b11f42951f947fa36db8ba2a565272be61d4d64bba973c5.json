async () => {\r\n        if (this.id) {\r\n          await APIService.put(`/ThongTinDanhMuc/edit/${this.id}`,this.chuyenmuc) \r\n          this.$message.success('Cập nhật huyện thành công')\r\n        }\r\n        else {\r\n           await APIService.post(\"ThongTinDanhMuc/create\",this.chuyenmuc)\r\n           console.log('chhuydsen muc', this.chuyenmuc)\r\n           this.$message.success('Thêm chuyên mục thành công')\r\n        }\r\n        this.closeModal()\r\n        this.$emit('addSuccess')\r\n      }).catch(error => {\r\n        console.log('error', error);\r\n      });\r\n    },\r\n    resetForm() {\r\n      this.$refs.formRef.resetFields();\r\n    },\r\n  }\r\n};\r\n</script>\r\n\r\n<style>\r\n</style>"],"mappings":"AA8BA,OAAOA,UAAS,MAAO,sBAAsB;AAC7C,eAAe;EACbC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,IAAI,EAAE,KAAK;MACXC,QAAQ,EAAE;QACRC,IAAI,EAAE;MACR,CAAC;MACDC,UAAU,EAAE;QACVD,IAAI,EAAE;MACR,CAAC;MACDE,EAAE,EAAEC,SAAS;MACbC,SAAS,EAAE;QACTC,MAAM,EAAE,EAAE;QACVC,UAAU,EAAC,EAAE;QACbC,YAAY,EAAE,EAAE;QAChBC,MAAM,EAAE,EAAE;QACVC,SAAS,EAAC;MACZ;IAEF,CAAC;EACH,CAAC;EACDC,OAAO,EAAC;IACN,MAAMC,SAASA,CAACT,EAAE,EAAE;MAClB,IAAI,CAACJ,IAAG,GAAI,IAAG;MACf,IAAGI,EAAE,EAAE;QACL,IAAIU,GAAE,GAAI,MAAMhB,UAAU,CAACiB,GAAG,CAAE,oBAAmBX,EAAG,EAAC;QACvD,IAAI,CAACA,EAAC,GAAIA,EAAC;QACX,IAAI,CAACE,SAAQ,GAAIQ,GAAG,CAACf,IAAI,CAACA,IAAG;MAC/B;IACF,CAAC;IACDiB,UAAUA,CAAA,EAAG;MACX,IAAI,CAAChB,IAAG,GAAI,KAAI;MAChB,IAAI,CAACI,EAAC,GAAIC,SAAQ;MAClB,IAAI,CAACY,SAAS,CAAC;IACjB,CAAC;IACDC,QAAQA,CAAA,EAAG;MACT,IAAI,CAACC,KAAK,CAACC,OAAO,CAACC,QAAQ,CAAC,CAAC,CAACC,IAAI,CAAC,YAAY;QAC7C,IAAI,IAAI,CAAClB,EAAE,EAAE;UACX,MAAMN,UAAU,CAACyB,GAAG,CAAE,yBAAwB,IAAI,CAACnB,EAAG,EAAC,EAAC,IAAI,CAACE,SAAS;UACtE,IAAI,CAACkB,QAAQ,CAACC,OAAO,CAAC,2BAA2B;QACnD,OACK;UACF,MAAM3B,UAAU,CAAC4B,IAAI,CAAC,wBAAwB,EAAC,IAAI,CAACpB,SAAS;UAC7DqB,OAAO,CAACC,GAAG,CAAC,eAAe,EAAE,IAAI,CAACtB,SAAS;UAC3C,IAAI,CAACkB,QAAQ,CAACC,OAAO,CAAC,4BAA4B;QACrD;QACA,IAAI,CAACT,UAAU,CAAC;QAChB,IAAI,CAACa,KAAK,CAAC,YAAY;MACzB,CAAC,CAAC,CAACC,KAAK