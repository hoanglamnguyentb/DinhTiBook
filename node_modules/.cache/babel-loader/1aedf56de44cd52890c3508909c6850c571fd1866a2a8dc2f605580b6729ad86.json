uc: {\r\n          required: helpers.withMessage(\r\n            \"Vui lòng chọn danh mục bài viết\",\r\n            required\r\n          ),\r\n        },\r\n        content: {\r\n          required: helpers.withMessage(\r\n            \"Vui lòng nhập nội dung bài viết\",\r\n            required\r\n          ),\r\n        },\r\n        trangThai: {\r\n          required: helpers.withMessage(\r\n            \"Vui lòng chọn trạng thái bài viết\",\r\n            required\r\n          ),\r\n        },\r\n      },\r\n    };\r\n  },\r\n  mounted() {\r\n    //this.getChuyenMuc()\r\n  },\r\n};\r\n</script>\r\n<style>\r\n.ql-container {\r\n  min-height: 300px;\r\n}\r\n</style>\r\n<template>\r\n  <PageHeader :title=\"title\" :items=\"items\" />\r\n  <a-form>\r\n    <a-row :gutter=\"20\">\r\n      <a-col :span=\"16\">\r\n        <a-card>\r\n          <template #title>\r\n            <h5 class=\"custom-card-title mb-0\">Thông tin bài viết</h5>\r\n          </template>\r\n          <div>\r\n            <a-form-item>\r\n              <label for=\"chuyenMuc\">Chuyên mục tin tức:</label>\r\n              <a-select\r\n                id=\"chuyenMuc\"\r\n                v-model=\"post.chuyenMuc\"\r\n                placeholder=\"Chọn chuyên mục\"\r\n                size=\"large\"\r\n              >\r\n                <a-select-option\r\n                  v-for=\"item in options\"\r\n                  :key=\"item.value\"\r\n                  :value=\"item.value\"\r\n                >\r\n                  {{ item.text }}\r\n                </a-select-option>\r\n              </a-select>\r\n            </a-form-item>\r\n            <a-form-item>\r\n              <label for=\"TieuDe\">Tiêu đề:</label>\r\n              <a-input size=\"large\" id=\"TieuDe\" v-model:value=\"post.username\" />\r\n            </a-form-item>\r\n            <a-form-item label=\"Nổi bật\">\r\n              <a-checkbox v-model=\"post.noiBat\"></a-checkbox>\r\n            </a-form-item>\r\n            <a-form-ite