ject = cart.find(o => o.id === this.SanPham.id);\n      if (object) {\n        console.log(\"Co san pham roi\");\n        this.$router.push(\"/gio-hang\");\n      } else {\n        this.SanPham.quantity = this.quantity;\n        this.$store.commit(\"addCart\", {\n          products: this.SanPham,\n          Add: this.Add\n        });\n        console.log(this.$store.state.cart);\n        this.$router.push(\"/gio-hang\");\n      }\n    }\n  },\n  mounted() {\n    this.getSanPhamById();\n  }\n};","map":{"version":3,"names":["Header","FooterClient","APIService","components","data","URL","SanPham","lstAnh","quantity","Add","methods","getSanPhamById","paramId","$route","params","id","console","log","get","then","response","objInfo","anhSanPham","pathAnh","path","TangSoLuong","GiamSoLuong","addCard","cart","$store","state","object","find","o","$router","push","commit","products","buyNow","mounted"],"sources":["D:\\DA\\DoAnFrontEnd\\DoAnClient\\client\\src\\views\\client\\DetailSanPham.vue"],"sourcesContent":["<template>\r\n  <Header></Header>\r\n  <div>\r\n    <div\r\n      style=\"padding-top: 50px; padding-bottom: 100px\"\r\n      v-if=\"SanPham\"\r\n      class=\"container\"\r\n    >\r\n      <a-row class=\"block-product\">\r\n        <a-col :span=\"10\">\r\n          <div style=\"width: 484px\" class=\"product-essential-media\">\r\n            <div style=\"display: flex\" class=\"product-view-image\">\r\n              <div class=\"product-view-thumbnail\">\r\n                <div class=\"list-anh\">\r\n                  <a v-for=\"(item, index) in lstAnh\" :key=\"index\" href=\"\">\r\n                    <img :src=\"URL + item.path\" />\r\n                  </a>\r\n                </div>\r\n              </div>\r\n\r\n              <div v-if=\"lstAnh && lstAnh.length > 0\" class=\"product-image\">\r\n                <img :src=\"URL + lstAnh[0].path\" alt=\"\" />\r\n              </div>\r\n            </div>\r\n          </div>\r\n        </a-col>\r\n        <a-col :sp