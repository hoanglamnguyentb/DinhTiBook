false,\r\n        dateFormat: \"d/m/Y\",\r\n        //time_24hr: true,\r\n      },\r\n      editorOptions: {\r\n        modules: {\r\n          toolbar: {\r\n            container: [\r\n              ['bold', 'italic', 'underline', 'strike'],        // toggled buttons\r\n              ['blockquote', 'code-block'],\r\n\r\n              [{ 'header': 1 }, { 'header': 2 }],               // custom button values\r\n              [{ 'list': 'ordered' }, { 'list': 'bullet' }],\r\n              [{ 'script': 'sub' }, { 'script': 'super' }],     // superscript/subscript\r\n              [{ 'indent': '-1' }, { 'indent': '+1' }],         // outdent/indent\r\n              [{ 'direction': 'rtl' }],                         // text direction\r\n\r\n              [{ 'size': ['small', false, 'large', 'huge'] }],  // custom dropdown\r\n              [{ 'header': [1, 2, 3, 4, 5, 6, false] }],\r\n\r\n              [{ 'color': [] }, { 'background': [] }],          // dropdown with defaults from theme\r\n              [{ 'font': [] }],\r\n              [{ 'align': [] }],\r\n              ['link', 'fileManager'],\r\n              ['clean']                                         // remove formatting button\r\n            ],\r\n            handlers: {\r\n              'fileManager': function(e) {\r\n                console.log(e)\r\n              }\r\n            },\r\n          }\r\n        },\r\n        placeholder: 'Nhập nội dung bài viết...',\r\n        theme: 'snow'\r\n      },\r\n      post:{\r\n        title : \"\",\r\n        chuyenMuc : \"\",\r\n        content : \"\",\r\n        trangThai : \"\",\r\n        publicDate : new Date(),\r\n        description: \"\",\r\n        noiBat: \"\",\r\n        hinhAnh:\"\",\r\n\r\n      },\r\n      chuyenmuc:[\r\n      ],\r\n      lstChuyenMuc:[],\r\n      submitted : false,\r\n      showFileManager: false,\r\n      errMessage: null,\r\n      isLoading: false,\r\n      acceptType: ['image/png', 'image/jpg', 'image/jpeg'],\r\n      selected: 'false',\r\n      options: [\r\n          { text: 'Không', value: 0 },\r\n          { text: 'Có', value: 1 },\r\n        ],\r\n        idDanhMuc: '03'\r\n    };\r\n  },\r\n  components: {\r\n    PageHeader,\r\n    Multiselect,\r\n    flatPickr,\r\n    QuillEditor,\r\n    FileManager\r\n  },\r\n  methods: {\r\n    deleteRecord(ele) {\r\n      ele.target.parentElement.parentElement.parentElement.remove();\r\n    },\r\n\r\n    async onSubmit(){\r\n      this.submitted = true;\r\n      this.isLoading = true;\r\n      this.v$.$touch();\r\n      const result = await this.v$.$validate();\r\n      if(!result){\r\n        this.isLoading = false;\r\n      }\r\n      try {\r\n        await APIService.post(\"tintuc/create\", this.post);\r\n        toast.success(\"Thêm bài viết thành công\",{\r\n          \"theme\": \"colored\",\r\n          autoClose: 2000\r\n        });\r\n      } catch (e) {\r\n        this.submitted = false;\r\n        this.isLoading = false;\r\n        if(e.status == 401){\r\n          toast.error(\"Bạn không có quyền truy cập.\",{\r\n            \"theme\": \"colored\",\r\n            autoClose: 2000\r\n          });\r\n        }else {\r\n          if(e.data && e.data.Message){\r\n            this.errMessage = e.data.Message;\r\n            toast.error(e.data.Message,{\r\n              \"theme\": \"colored\",\r\n              autoClose: 2000\r\n            });\r\n          }else{\r\n            toast.error(\"Thêm mới bài viết thất bại. Vui lòng thử lại sau.\",{\r\n              \"theme\": \"colored\",\r\n              autoClose: 2000\r\n            });\r\n          }\r\n        }\r\n      }\r\n    },\r\n\r\n    handleSelectFile(data){\r\n      console.log(data)\r\n    }\r\n  },\r\n  validations () {\r\n    return {\r\n      post:{\r\n        title: {\r\n          required: helpers.withMessage(\"Tiêu đề bài viết không được để trống\", required),\r\n        },\r\n        chuyenMuc: {\r\n          required: helpers.withMessage(\"Vui lòng chọn danh mục bài viết\", required),\r\n        },\r\n        content: {\r\n          required: helpers.withMessage(\"Vui lòng nhập nội dung bài viết\", required),\r\n        },\r\n        trangThai: {\r\n          required: helpers.withMessage(\"Vui lòng chọn trạng thái bài viết\", required),\r\n        },\r\n      }\r\n    }\r\n  },\r\n  mounted(){\r\n    //this.getChuyenMuc()\r\n  }\r\n};\r\n</script>\r\n<style>\r\n.ql-container{\r\n  min-height: 300px;\r\n}\r\n</style>\r\n<template>\r\n  <PageHeader :title=\"title\" :items=\"items\" />\r\n  <a-row :gutter=\"20\">\r\n    <!-- <b-col lg=\"8\">\r\n      <form @submit.prevent=\"onSubmit\">\r\n        <b-card no-body>\r\n          <b-card-header>\r\n            <h5 class=\"card-title mb-0\">Thông tin bài viết</h5>\r\n          </b-card-header>\r\n          <b-card-body>\r\n            <div>\r\n              <label class=\"form-label\" for=\"title\">Chuyên mục tin tức</label>\r\n              <Multiselect class=\"form-control\"\r\n              :class=\"{\r\n                  'is-invalid': submitted && v$.post.chuyenMuc.$error,\r\n                }\"\r\n              v-model=\"post.chuyenMuc\" value=\"0\" :close-on-select=\"true\" :searchable=\"true\"\r\n                :create-option=\"true\" :options=\"lstChuyenMuc\" />\r\n              <div v-for=\"(item, index) in v$.post.chuyenMuc.$errors\" :key=\"index\" class=\"invalid-feedback\">\r\n                <span v-if=\"item.$message\">{{ item.$message }}</span>\r\n              </div>\r\n            </div>\r\n            <div class=\"mb-3\">\r\n              <label class=\"form-label\" for=\"title\">Tiêu đề</label>\r\n              <input type=\"text\" class=\"form-control\" \r\n              :class=\"{\r\n                'is-invalid': submitted && v$.post.title.$error,\r\n              }\" id=\"product-title-input\" v-model=\"post.title\" placeholder=\"Nhập tiêu đề bài viết\" />\r\n              <div v-for=\"(item, index) in v$.post.title.$errors\" :key=\"index\" class=\"invalid-feedback\">\r\n                <span v-if=\"item.$message\">{{ item.$message }}</span>\r\n              </div>\r\n            </div>\r\n\r\n            <div>\r\n              <b-form-group label=\"Nổi bật\" v-slot=\"{ ariaDescribedby }\">\r\n                <b-form-radio-group\r\n                  v-model=\"post.noiBat\"\r\n                  :options=\"options\"\r\n                  :aria-describedby=\"ariaDescribedby\"\r\n                  name=\"radio-inline\"\r\n                ></b-form-radio-group>\r\n                \r\n              </b-form-group>\r\n\r\n              <div class=\"mt-3\">Selected: <strong>{{ post.noiBat }}</strong></div>\r\n            </div>\r\n            <div>\r\n              <label>Nội dung bài viết</label>\r\n              <QuillEditor \r\n              theme=\"snow\" \r\n              :options=\"editorOptions\"\r\n              v-model:content=\"post.content\"\r\n              contentType=\"html\" />\r\n              <div v-for=\"(item, index) in v$.post.content.$errors\" :key=\"index\" class=\"invalid-feedback\">\r\n                <span v-if=\"item.$message\">{{ item.$message }}</span>\r\n              </div>\r\n            </div>\r\n\r\n            <div class=\"mt-2\">\r\n              <label>Mô tả bài viết</label>\r\n              <textarea class=\"form-control\" v-model=\"post.description\" placeholder=\"Nhập mô tả\" rows=\"3\"></textarea>\r\n            </div>\r\n          </b-card-body>\r\n        </b-card>\r\n\r\n        <b-card no-body>\r\n          <b-card-header>\r\n            <h5 class=\"card-title mb-0\">Ảnh đại diện</h5>\r\n          </b-card-header>\r\n          <b-card-body>\r\n            <div class=\"vstack gap-2\">\r\n              <FileManager \r\n              :active=\"showFileManager\" \r\n              :showButton=\"true\"\r\n              :showPreview=\"true\"\r\n              :closable=\"false\"\r\n              :maxCount=\"1\"\r\n              :multiple=\"false\"\r\n              :accept=\"acceptType\"\r\n              @select-file=\"handleSelectFile\"\r\n               />\r\n            </div>\r\n          </b-card-body>\r\n        </b-card>\r\n\r\n        <div class=\"text-end mb-3\">\r\n          <b-button variant=\"success\" class=\"w-sm\" type=\"submit\" :disable=\"isLoading\">\r\n            <span v-if=\"isLoading\">\r\n              <b-spinner small></b-spinner>\r\n              Đang xử lý...\r\n            </span>\r\n            <span v-if=\"!isLoading\">Lưu</span>\r\n          </b-button>\r\n        </div>\r\n      </form>\r\n    </b-col> -->\r\n    \r\n    <a-col :span=\"16\" >\r\n      <a-form>\r\n        <a-card>\r\n        <template #title>\r\n          <h5 class=\"custom-card-title mb-0\">Thông tin bài viết</h5>\r\n        </template>\r\n            <div>\r\n              <a-form-item >\r\n                <label  for=\"chuyenMuc\">Chuyên mục tin tức:</label>\r\n                <a-select \r\n                id=\"chuyenMuc\"\r\n                  v-model=\"post.chuyenMuc\" \r\n                  placeholder=\"Chọn chuyên mục\" \r\n                  size=\"large\"\r\n                >\r\n                  <a-select-option \r\n                    v-for=\"item in options\" \r\n                    :key=\"item.value\" \r\n                    :value=\"item.value\"\r\n                  >\r\n                    {{ item.text }}\r\n                  </a-select-option>\r\n                </a-select>\r\n              </a-form-item>\r\n              <a-form-item>\r\n                <label for=\"TieuDe\">Tiêu đề:</label>\r\n                <a-input size=\"large\" id=\"TieuDe\" v-model:value=\"post.username\" />\r\n              </a-form-item>\r\n              <a-form-item label=\"Nổi bật\">\r\n                <a-checkbox v-model=\"post.noiBat\"></a-checkbox>\r\n              </a-form-item>\r\n              <a-form-item>\r\n                <label for=\"NoiDung\">Nội dung bài viết:</label>\r\n                <QuillEditor \r\n                theme=\"snow\" \r\n                :options=