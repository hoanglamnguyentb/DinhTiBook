   items: [\r\n        {\r\n          text: \"Dashboard\",\r\n          href: \"/\",\r\n        },\r\n        {\r\n          text: \"Quản lý chức năng\",\r\n          active: true,\r\n        },\r\n      ],\r\n      tableColumns: [\r\n        { title: \"Mã thao tác\", dataIndex: \"maThaoTac\", key: \"ma\" },\r\n        { title: \"Tên thao tác\", dataIndex: \"tenThaoTac\", key: \"ten\" },\r\n        { title: \"Thứ tự\", dataIndex: \"thuTu\", key: \"thuTu\" },\r\n        { title: \"Đường dẫn\", dataIndex: \"duongDan\", key: \"duongDan\" },\r\n        { title: \"Thao tác\", key: \"actions\", width: 200 },\r\n      ],\r\n      searchQuery: null,\r\n      pages: [],\r\n      lstChucNang: [],\r\n      optionPage: {\r\n        pageIndex: 1,\r\n        pageSize: 10,\r\n        totalCount: 0,\r\n      },\r\n      formSearch: {\r\n        maFilter: \"\",\r\n        tenFilter: \"\",\r\n      },\r\n      chucNangId: this.$route.params.chucNangId\r\n    };\r\n  },\r\n  methods: {\r\n    formatDate: Common.formatDate,\r\n    openModalAdd() {\r\n      this.$refs.modalAddThaoTac.showModal();\r\n    },\r\n    openModalEdit(record) {\r\n      this.$refs.modalAddThaoTac.showModal(record);\r\n    },\r\n    handlePageChange(page) {\r\n      this.optionPage.pageIndex = page;\r\n      this.loadData(this.optionPage.pageIndex, this.optionPage.pageSize);\r\n    },\r\n    SearchData() {\r\n      var searchParam = {\r\n        pageIndex: this.optionPage.pageIndex,\r\n        pageSize: this.optionPage.pageSize,\r\n        tenFilter: this.formSearch.tenFilter,\r\n        maFilter: this.formSearch.maFilter,\r\n      };\r\n\r\n      this.loadData(1, 10, searchParam);\r\n    },\r\n    async loadData(pageIndex, pageSize, params) {\r\n      var searchParam = {\r\n        pageIndex: pageIndex,\r\n        pageSize: pageSize,\r\n      };\r\n      if (params) {\r\n        searchParam = params;\r\n      }\r\n      var urlQuery = new URLSearchParams(searchParam).toString();\r\n      var result = (await APIService.get(`/ThaoTac/GetListThaoTacByChucNangId/${this.chucNangId}?${urlQuery}`)).data;\r\n      this.lstChucNang = result.data.items;\r\n      if (result.data != null && result.data.items != null) {\r\n        var res = result.data;\r\n        this.lstChucNang = result.data.items;\r\n        this.optionPage.pageIndex = res.pageIndex;\r\n        this.optionPage.pageSize = res.pageSize;\r\n        this.optionPage.totalCount = res.totalCount;\r\n      }\r\n    },\r\n    async deleteChucNang(id) {\r\n      var res = await APIService.delete(`/ThaoTac/${id}`);\r\n      if (res.data.status !== \"Success\") {\r\n        this.$message.error();\r\n        return;\r\n      }\r\n      this.$message.success(\"Xóa thao tác thành công\");\r\n      this.loadData(this.optionPage.pageIndex, this.optionPage.pageSize);\r\n    },\r\n  }\r\n}\r\n</script>\r\n"],"mappings":"AAuHA,OAAOA,UAAS,MAAO,0BAA0B;AACjD,OAAOC,SAAQ,MAAO,wBAAwB;AAC9C,OAAOC,UAAS,MAAO,4CAA4C;AACnE,OAAOC,UAAS,MAAO,sBAAqB;AAC5C;AACA,OAAOC,MAAK,MAAO,kBAAiB;AACpC,OAAOC,mBAAkB,MAAO,2BAA2B;AAE3D,eAAe;EACbC,OAAOA,CAAA,EAAG;IACR,IAAI,CAACC,QAAQ,CAAC,IAAI,CAACC,UAAU,CAACC,SAAS,EAAE,IAAI,CAACD,UAAU,CAACE,QAAQ,CAAC;EAEpE,CAAC;EACDC,UAAU,EAAE;IACVX,UAAU;IACVE,UAAU;IACVG;EACF,CAAC;EACDO,IAAI,EAAE;IACJC,KAAK,EAAE,mBAAmB;IAC1BC,IAAI,EAAE,CACJ;MACEC,IAAI,EAAE,mBAAmB;MACzBC,OAAO,EAAEf,SAAS,CAACgB;IACrB,CAAC;EAEL,CAAC;EACDC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,SAAS,EAAE,GAAG;MACdN,KAAK,EAAE,mBAAmB;MAC1BO,KAAK,EAAE,CACL;QACEC,IAAI,EAAE,WAAW;QACjBC,IAAI,EAAE;MACR,CAAC,EACD;QACED,IAAI,EAAE,mBAAmB;QACzBE,MAAM,EAAE;MACV,CAAC,CACF;MACDC,YAAY,EAAE,CACZ;QAAEX,KAAK,EAAE,aAAa;QAAEY,SAAS,EAAE,WAAW;QAAEC,GAAG,EAAE;MAAK,CAAC,EAC3D;QAAEb,KAAK,EAAE,cAAc;QAAEY,SAAS,EAAE,YAAY;QAAEC,GAAG,EAAE;MAAM,CAAC,EAC9D;QAAEb,KAAK,EAAE,QAAQ;QAAEY,SAAS,EAAE,OAAO;QAAEC,GAAG,EAAE;MAAQ,CAAC,EACrD;QAAEb,KAAK,EAAE,WAAW;QAAEY,SAAS,EAAE,UAAU;QAAEC,GAAG,EAAE;MAAW,CAAC,EAC9D;QAAEb,KAAK,EAAE,UAAU;QAAEa,GAAG,EAAE,SAAS;QAAEC,KAAK,EAAE;MAAI,CAAC,CAClD;MACDC,WAAW,EAAE,IAAI;MACjBC,KAAK,