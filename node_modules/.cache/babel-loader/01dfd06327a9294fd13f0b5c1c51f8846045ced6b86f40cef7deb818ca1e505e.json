{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport APIService from \"@/helpers/ApiService\";\nexport default {\n  props: {\n    parentId: ''\n  },\n  data() {\n    return {\n      previewVisible: false,\n      previewImage: '',\n      previewTitle: '',\n      fileList: [],\n      uploading: false\n    };\n  },\n  methods: {\n    getBase64(img, callback) {\n      const reader = new FileReader();\n      reader.addEventListener('load', () => callback(reader.result));\n      reader.readAsDataURL(img);\n    },\n    handleCancel() {\n      this.previewVisible = false;\n      this.previewTitle = '';\n      this.previewImage = '';\n      this.previewType = '';\n    },\n    async handlePreview(file) {\n      console.log('preview', file);\n\n      // Hàm lấy phần mở rộng của tệp\n      const getFileExtension = filename => {\n        return filename.split('.').pop().toLowerCase();\n      };\n      const fileExtension = getFileExtension(file.name || file.url);\n      console.log('k', fileExtension);\n      if (fileExtension === 'pdf') {\n        // Xử lý xem trước PDF\n        this.previewImage = file.url || URL.createObjectURL(file.file);\n        this.previewVisible = true;\n        this.previewType = 'pdf';\n        this.previewTitle = file.name || file.url.substring(file.url.lastIndexOf('/') + 1);\n      } else {\n        if (!file.url && !file.preview) {\n          this.getBase64(file.file, preview => {\n            file.preview = preview;\n            this.previewImage = file.file.url || file.file.preview;\n            this.previewVisible = true;\n            this.previewType = 'image';\n            this.previewTitle = file.file.name || file.file.url.substring(file.url.lastIndexOf('/') + 1);\n          });\n        } else {\n          this.previewImage = file.url || file.preview;\n          this.previewVisible = true;\n          this.previewType = 'image';\n          this.previewTitle = file.name || file.url.substring(file.url.lastIndexOf('/') + 1);\n        }\n      }\n    },\n    async beforeUpload(file) {\n      // this.fileList = [...(this.fileList || []), file];\n      file.url = URL.createObjectURL(file);\n      file.status = 'done';\n      this.fileList.push(file);\n      console.log('file11', this.fileList);\n    },\n    async handleUpload(newProductId) {\n      console.log('Đã nhận được newProductId: ', newProductId);\n      const formData = new FormData();\n      // this.fileList.forEach(file => {\n      //   formData.append('Files[]', file);\n      // });\n      // formData.append('Files', this.fileList)\n\n      this.fileList.forEach(file => {\n        formData.append('Files', file);\n      });\n      this.uploading = true;\n      // APIService.post(`/FileManager/Upload1?ParentID=${this.parentId}`, formData)\n      APIService.post(`/FileManager/Upload1?ParentID=${newProductId}`, formData).then(() => {\n        // fileList.value = [];\n        // this.uploading = false;\n\n        // this.$message.success('Cập nhật thành công')\n        this.fileList = [];\n        console.log('thanhf cong');\n      }).catch(() => {\n        this.uploading = false;\n        // message.error('upload failed.');\n        // this.$message.error('Cập nhật lỗi')\n      });\n    },\n    handleRemove(file) {\n      const index = this.fileList.indexOf(file);\n      this.fileList.splice(index, 1);\n      console.log('xóa', this.fileList);\n    }\n  }\n};","map":{"version":3,"names":["APIService","props","parentId","data","previewVisible","previewImage","previewTitle","fileList","uploading","methods","getBase64","img","callback","reader","FileReader","addEventListener","result","readAsDataURL","handleCancel","previewType","handlePreview","file","console","log","getFileExtension","filename","split","pop","toLowerCase","fileExtension","name","url","URL","createObjectURL","substring","lastIndexOf","preview","beforeUpload","status","push","handleUpload","newProductId","formData","FormData","forEach","append","post","then","catch","handleRemove","index","indexOf","splice"],"sources":["D:\\DA\\DoAnFrontEnd\\DoAnClient\\client\\src\\components\\FileManager\\File_Manager.vue"],"sourcesContent":["<template>\r\n  <div class=\"\">\r\n    \r\n    <a-upload \r\n      :file-list=\"fileList\" \r\n      :before-upload=\"beforeUpload\" \r\n      @remove=\"handleRemove\"\r\n      @preview=\"handlePreview\"\r\n      list-type=\"picture-card\">\r\n\r\n      <div v-if=\"fileList.length < 6\">\r\n        <plus-outlined />\r\n        <div style=\"margin-top: 8px\">Tải ảnh lên</div>\r\n      </div>\r\n    </a-upload>\r\n    \r\n\r\n    <a-modal :open=\"previewVisible\" :title=\"previewTitle\" :footer=\"null\" @cancel=\"handleCancel\">\r\n      <div v-if=\"previewVisible\">\r\n      <h3>{{ previewTitle }}</h3>\r\n      <div v-if=\"previewType === 'image'\">\r\n        <img :src=\"previewImage\" alt=\"preview\" style=\"width: 100%\" />\r\n      </div>\r\n      <div v-if=\"previewType === 'pdf'\">\r\n        <iframe :src=\"previewImage\" width=\"100%\" height=\"600px\"></iframe>\r\n      </div>\r\n      <button @click=\"handleCancel\">Đóng</button>\r\n    </div>\r\n\r\n      <!-- <img alt=\"ví dụ\" style=\"width: 100%\" :src=\"previewImage\" /> -->\r\n    </a-modal> \r\n  </div>\r\n\r\n\r\n  \r\n</template>\r\n\r\n<script>\r\nimport APIService from \"@/helpers/ApiService\";\r\n\r\nexport default {\r\n  props:{\r\n    parentId :'',\r\n \r\n  },\r\n  data() {\r\n    return {\r\n      previewVisible: false,\r\n      previewImage: '',\r\n      previewTitle: '',\r\n      fileList: [],\r\n      uploading: false,\r\n      \r\n    };\r\n  },\r\n  methods: {\r\n    getBase64(img, callback) {\r\n      const reader = new FileReader();\r\n      reader.addEventListener('load', () => callback(reader.result));\r\n      reader.readAsDataURL(img);\r\n    },\r\n    handleCancel() {\r\n      this.previewVisible = false;\r\n      this.previewTitle = '';\r\n      this.previewImage = '';\r\n      this.previewType = '';\r\n    },\r\n    async handlePreview(file) {\r\n      console.log('preview', file)\r\n\r\n       // Hàm lấy phần mở rộng của tệp\r\n      const getFileExtension = (filename) => {\r\n        return filename.split('.').pop().toLowerCase();\r\n      };\r\n\r\n      const fileExtension = getFileExtension(file.name || file.url);\r\n      console.log('k', fileExtension)\r\n\r\n      if (fileExtension === 'pdf') {\r\n        // Xử lý xem trước PDF\r\n        this.previewImage = file.url || URL.createObjectURL(file.file);\r\n        this.previewVisible = true;\r\n        this.previewType = 'pdf';\r\n        this.previewTitle = file.name || file.url.substring(file.url.lastIndexOf('/') + 1);\r\n      }\r\n      else{\r\n\r\n      if (!file.url && !file.preview) {\r\n        this.getBase64(file.file, (preview) => {\r\n          file.preview = preview;\r\n          this.previewImage = file.file.url || file.file.preview;\r\n          this.previewVisible = true;\r\n          this.previewType = 'image';\r\n          this.previewTitle = file.file.name || file.file.url.substring(file.url.lastIndexOf('/') + 1);\r\n        });\r\n      } else {\r\n        this.previewImage = file.url || file.preview;\r\n        this.previewVisible = true;\r\n        this.previewType = 'image';\r\n        this.previewTitle = file.name || file.url.substring(file.url.lastIndexOf('/') + 1);\r\n      }\r\n    }\r\n    },\r\n    \r\n    async beforeUpload(file){\r\n      \r\n      // this.fileList = [...(this.fileList || []), file];\r\n      file.url = URL.createObjectURL(file);\r\n      file.status = 'done';\r\n      this.fileList.push(file);\r\n   \r\n      \r\n      console.log('file11', this.fileList)\r\n    },\r\n\r\n    async handleUpload(newProductId){\r\n      console.log('Đã nhận được newProductId: ', newProductId);\r\n      const formData = new FormData();\r\n      // this.fileList.forEach(file => {\r\n      //   formData.append('Files[]', file);\r\n      // });\r\n      // formData.append('Files', this.fileList)\r\n\r\n      this.fileList.forEach(file => {\r\n        formData.append('Files', file);\r\n      });\r\n\r\n      this.uploading = true;\r\n      // APIService.post(`/FileManager/Upload1?ParentID=${this.parentId}`, formData)\r\n      APIService.post(`/FileManager/Upload1?ParentID=${newProductId}`, formData)\r\n        .then(() => {\r\n          // fileList.value = [];\r\n          // this.uploading = false;\r\n  \r\n          // this.$message.success('Cập nhật thành công')\r\n          this.fileList = [];\r\n          console.log('thanhf cong')\r\n        })\r\n        .catch(() => {\r\n          this. uploading = false;\r\n          // message.error('upload failed.');\r\n          // this.$message.error('Cập nhật lỗi')\r\n        });\r\n    },\r\n    handleRemove(file) {\r\n      const index = this.fileList.indexOf(file);\r\n      this.fileList.splice(index, 1);\r\n      console.log('xóa', this.fileList)\r\n    },\r\n\r\n  }\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.ant-upload-select-picture-card i {\r\n  font-size: 32px;\r\n  color: #999;\r\n}\r\n\r\n.ant-upload-select-picture-card .ant-upload-text {\r\n  margin-top: 8px;\r\n  color: #666;\r\n}\r\n</style>\r\n\r\n\r\n\r\n"],"mappings":";AAsCA,OAAOA,UAAS,MAAO,sBAAsB;AAE7C,eAAe;EACbC,KAAK,EAAC;IACJC,QAAO,EAAG;EAEZ,CAAC;EACDC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,cAAc,EAAE,KAAK;MACrBC,YAAY,EAAE,EAAE;MAChBC,YAAY,EAAE,EAAE;MAChBC,QAAQ,EAAE,EAAE;MACZC,SAAS,EAAE;IAEb,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACPC,SAASA,CAACC,GAAG,EAAEC,QAAQ,EAAE;MACvB,MAAMC,MAAK,GAAI,IAAIC,UAAU,CAAC,CAAC;MAC/BD,MAAM,CAACE,gBAAgB,CAAC,MAAM,EAAE,MAAMH,QAAQ,CAACC,MAAM,CAACG,MAAM,CAAC,CAAC;MAC9DH,MAAM,CAACI,aAAa,CAACN,GAAG,CAAC;IAC3B,CAAC;IACDO,YAAYA,CAAA,EAAG;MACb,IAAI,CAACd,cAAa,GAAI,KAAK;MAC3B,IAAI,CAACE,YAAW,GAAI,EAAE;MACtB,IAAI,CAACD,YAAW,GAAI,EAAE;MACtB,IAAI,CAACc,WAAU,GAAI,EAAE;IACvB,CAAC;IACD,MAAMC,aAAaA,CAACC,IAAI,EAAE;MACxBC,OAAO,CAACC,GAAG,CAAC,SAAS,EAAEF,IAAI;;MAE1B;MACD,MAAMG,gBAAe,GAAKC,QAAQ,IAAK;QACrC,OAAOA,QAAQ,CAACC,KAAK,CAAC,GAAG,CAAC,C