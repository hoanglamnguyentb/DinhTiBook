ch(file => {\r\n      //   formData.append('Files[]', file);\r\n      // });\r\n      // formData.append('Files', this.fileList)\r\n\r\n      this.fileList.forEach(file => {\r\n        formData.append('Files', file);\r\n      });\r\n\r\n      this.uploading = true;\r\n      // APIService.post(`/FileManager/Upload1?ParentID=${this.parentId}`, formData)\r\n      APIService.post(`/FileManager/Upload1?ParentID=${newProductId}`, formData)\r\n        .then(() => {\r\n          // fileList.value = [];\r\n          // this.uploading = false;\r\n  \r\n          // this.$message.success('Cập nhật thành công')\r\n          this.fileList = [];\r\n          console.log('thanhf cong')\r\n        })\r\n        .catch(() => {\r\n          this. uploading = false;\r\n          // message.error('upload failed.');\r\n          // this.$message.error('Cập nhật lỗi')\r\n        });\r\n    },\r\n    handleRemove(file) {\r\n      const index = this.fileList.indexOf(file);\r\n      this.fileList.splice(index, 1);\r\n      console.log('xóa', this.fileList)\r\n    },\r\n\r\n  }\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.ant-upload-select-picture-card i {\r\n  font-size: 32px;\r\n  color: #999;\r\n}\r\n\r\n.ant-upload-select-picture-card .ant-upload-text {\r\n  margin-top: 8px;\r\n  color: #666;\r\n}\r\n</style>\r\n\r\n\r\n\r\n"],"mappings":";;;EACOA,KAAK,EAAC;AAAE;;;;gEAcPC,mBAAA,CAA8C;EAAzCC,KAAuB,EAAvB;IAAA;EAAA;AAAuB,GAAC,aAAW;;;;;;uBAd9CC,mBAAA,CAgCM,OAhCNC,UAgCM,GA9BJC,YAAA,CAcWC,mBAAA;IAbR,WAAS,EAAEC,KAAA,CAAAC,QAAQ;IACnB,eAAa,EAAEC,QAAA,CAAAC,YAAY;IAC3BC,QAAM,EAAEF,QAAA,CAAAG,YAAY;IACpBC,SAAO,EAAEJ,QAAA,CAAAK,aAAa;IACvB,WAAS,EAAC;;sBACV,MAGe,CAHfC,mBAAA,2GAGe,EACJR,KAAA,CAAAC,QAAQ,CAACQ,MAAM,Q,cAA1Bb,mBAAA,CAGM,OAAAc,UAAA,GAFJZ,YAAA,CAAiBa,wBAAA,GACjBC,UAA8C,C;;8EAIlDJ,mBAAA,8QAQc,EAGdV,YAAA,CAEUe,kBAAA;IAFAC,IAAI,EAAEd,KAAA,CAAAe,cAAc;IAAGC,KAAK,EAAEhB,KAAA,CAAAiB,YAAY;IAAGC,MAAM,EAAE,IAAI;IAAGC,QAAM,EAAEjB,QAAA,CAAAkB;;sBAC5E,MAA2D,CAA3D1B,mBAAA,CAA2D;MAAtD2B,GAAG,EAAC,OAAO;MAAC1B