             indent: \"-1\"\n            }, {\n              indent: \"+1\"\n            }],\n            // outdent/indent\n            [{\n              direction: \"rtl\"\n            }],\n            // text direction\n\n            [{\n              size: [\"small\", false, \"large\", \"huge\"]\n            }],\n            // custom dropdown\n            [{\n              header: [1, 2, 3, 4, 5, 6, false]\n            }], [{\n              color: []\n            }, {\n              background: []\n            }],\n            // dropdown with defaults from theme\n            [{\n              font: []\n            }], [{\n              align: []\n            }], [\"link\", \"fileManager\", \"image\"], [\"clean\"] // remove formatting button\n            ],\n            handlers: {\n              fileManager: function (e) {\n                console.log('sadsa', e);\n              }\n            }\n          }\n        },\n        options: {\n          upload: file => {\n            return new Promise((resolve, reject) => {\n              const formData = new FormData();\n              formData.append(\"image\", file);\n              axios.post('http://localhost:44301/api/FileAndFolder/Upload', formData).then(res => {\n                console.log('haha', res);\n                resolve(res.data.url);\n              }).catch(err => {\n                reject(\"Upload failed\");\n                console.error(\"Error:\", err);\n              });\n            });\n          }\n        },\n        placeholder: \"Nhập nội dung bài viết...\",\n        theme: \"snow\"\n      },\n      post: {\n        title: \"\",\n        chuyenMuc: \"\",\n        content: \"\",\n        trangThai: \"\",\n        publicDate: new Date(),\n        description: \"\",\n        noiBat: 0,\n        hinhAnh: \"\",\n        henGioDang: new Date(),\n        danhMuc: \"\",\n        fileDinhKem: \"\"\n      },\n      chuyenmuc: [],\n      lstChuyenMuc: [],\n      submitted: false,\n      showFileManager: false,\n      errMessage: null,\n      isLoading: false,\n      acceptType: [\"image/png\", \"image/jpg\", \"image/jpeg\"],\n      selected: \"false\",\n      options: [{\n        text: \"Không\",\n        value: 0\n      }, {\n        text: \"Có\",\n        value: 1\n      }],\n      idDanhMuc: \"03\",\n      rules: {\n        TieuDe: [{\n          required: true,\n          message: 'Please input Activity name',\n          trigger: 'change'\n        }]\n      },\n      checked: false,\n      open: false\n    };\n  },\n  components: {\n    PageHeader,\n    Multiselect,\n    flatPickr,\n    QuillEditor,\n    FileManager\n  },\n  methods: {\n    deleteRecord(ele) {\n      ele.target.parentElement.parentElement.parentElement.remove();\n    },\n    async onSubmit() {\n      try {\n        this.post.henGioDang.setHours(0, 0, 0, 0);\n        console.log('post', this.post);\n        await APIService.post(\"tintuc/create\", this.post);\n        toast.success(\"Thêm bài viết thành công\", {\n          theme: \"colored\",\n          autoClose: 2000\n        });\n      } catch (e) {\n        this.submitted = false;\n        this.isLoading = false;\n        if (e.response && e.response.status === 400 && e.response.data.errors) {\n          const {\n            errors\n          } = e.response.data;\n          // Xử lý lỗi cụ thể\n          if (errors.noiBat) {\n            toast.error(errors.noiBat[0], {\n              theme: \"colored\",\n              autoClose: 2000\n            });\n          }\n          if (errors.tintuc) {\n            toast.error(errors.tintuc[0], {\n              theme: \"colored\",\n              autoClose: 2000\n            });\n          }\n        } else {\n          console.error('Lỗi không xác định:', e);\n          toast.error(\"Có lỗi xảy ra. Vui lòng thử lại sau.\", {\n            theme: \"colored\",\n            autoClose: 2000\n          });\n        }\n      }\n    },\n    handleCheckboxChange() {\n      // Kiểm tra nếu checkbox được chọn\n      if (this.checked) {\n        this.post.noiBat = 1; // Thiết lập post.noiBat = 1\n      } else {\n        this.post.noiBat = 0; // Thiết lập post.noiBat = 0\n      }\n    },\n    async getChuyenMuc() {\n      var result = await APIService.get(`/ThongTinDanhMuc/GetDataByPage?MaDanhMuc=` + this.idDanhMuc);\n      this.chuyenmuc = result.data.data.items;\n      for (var i = 0; i < this.chuyenmuc.length; i++) {\n        var chuyenmucItem = {\n          value: this.chuyenmuc[i].giaTri,\n          label: this.chuyenmuc[i].giaTri\n        };\n        this.lstChuyenMuc.push(chuyenmucItem);\n      }\n    },\n    handleSelectFile(data) {\n      console.log('ewww', data.name);\n      this.post.hinhAnh = data.name;\n      console.log('sss', this.post.hinhAnh);\n    },\n    handleSelectFile_DinhKem(data) {\n      this.post.fileDinhKem = data.name;\n      console.log('file', this.post.fileDinhKem);\n    },\n    async submit() {\n      this.$refs.formRef.validate(valid => {\n        if (valid) {\n          console.log('values', this.formState);\n        } else {\n          console.log('error');\n        }\n      });\n    }\n  },\n  quill(content) {\n    console.log('Nội dung mới:', content);\n  },\n  validations() {\n    return {\n      post: {\n        title: {\n          required: helpers.withMessage(\"Tiêu đề bài viết không được để trống\", required)\n        },\n        chuyenMuc: {\n          required: helpers.withMessage(\"Vui lòng chọn danh mục bài viết\", required)\n        },\n        content: {\n          required: helpers.withMessage(\"Vui lòng nhập nội dung bài viết\", required)\n        },\n        trangThai: {\n          required: helpers.withMessage(\"Vui lòng chọn trạng thái bài viết\", required)\n        }\n      }\n    };\n  },\n  mounted() {\n    this.getChuyenMuc();\n  }\n};","map":{"version":3,"names":["ref","watch","Multiselect","useVuelidate","required","helpers","flatPickr","appConfig","PageHeader","QuillEditor","FileManager","APIService","toa