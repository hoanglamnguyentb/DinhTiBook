 false;\n          duplicateOrder = response.data.message == \"Thứ tự hiển thị bị trùng\" ? true : false;\n          console.log('chhuydsen muc', this.chuyenmuc);\n        }\n        if (duplicateCategory == false && duplicateOrder == false) {\n          this.closeModal();\n          this.$message.success(\"Thành công\");\n          this.$emit('addSuccess');\n        } else if (duplicateCategory) {\n          this.isLoading = false;\n          this.$message.error(\"Trùng mã danh mục\");\n        } else {\n          this.isLoading = false;\n          this.$message.error(\"Trùng thứ tự hiển thị\");\n        }\n        this.closeModal();\n        this.$emit('addSuccess');\n      }).catch(error => {\n        console.log('error', error);\n      });\n    },\n    resetForm() {\n      this.$refs.formRef.resetFields();\n    }\n  }\n};","map":{"version":3,"names":["APIService","integer","data","open","labelCol","span","wrapperCol","id","undefined","chuyenmuc","giaTri","maThongTin","thuTuHienThi","ghiChu","maDanhMuc","rules","required","message","trigger","type","methods","showModal","res","get","closeModal","resetForm","handleOk","$refs","formRef","validate","then","duplicateCategory","duplicateOrder","put","response","post","console","log","$message","success","$emit","isLoading","error","catch","resetFields"],"sources":["D:\\HINET_1\\BacKan\\client\\src\\views\\dashboard\\QL_ChuyenMucTinTuc\\ModalAddEditChuyenMucTT.vue"],"sourcesContent":["<template>\r\n  <div>\r\n    <a-modal\r\n      v-model:open=\"open\"\r\n      :title=\"id ? 'Cập nhật chuyên mục' : 'Thêm chuyên mục'\"\r\n      @ok=\"handleOk\"\r\n      @cancel=\"closeModal\"\r\n    >\r\n      <a-form ref=\"formRef\" :model=\"chuyenmuc\"  layout=\"vertical\"  :rules=\"rules\">\r\n        <a-form-item ref=\"giaTri\" label=\"Mã chuyên mục\" name=\"maThongTin\">\r\n          <a-input v-model:value=\"chuyenmuc.maThongTin\" />\r\n        </a-form-item>\r\n        <a-form-item ref=\"giaTri\" label=\"Tên chuyên mục\" name=\"giaTri\">\r\n          <a-input v-model:value=\"chuyenmuc.giaTri\" />\r\n        </a-form-item>\r\n        <a-form-item ref=\"thuTuHienThi\" label=\"Thứ tự hiển thị\" name=\"thuTuHienThi\">\r\n          <a-input v-model:value=\"chuyenmuc.thuTuHienThi\" />\r\n        </a-form-item>\r\n        <a-form-item ref=\"ghiChu\" label=\"Ghi chú\" name=\"ghiChu\">\r\n          <!-- <a-input v-model:value=\"chuyenmuc.ghiChu\" /> -->\r\n          <a-textarea :rows=\"4\" v-model:value=\"chuyenmuc.ghiChu\"></a-textarea>\r\n        </a-form-item>\r\n        \r\n        \r\n      </a-form>\r\n    </a-modal>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport APIService from \"@/helpers/ApiService\";\r\nimport { integer } from '@vuelidate/validators';\r\nexport default {\r\n  data() {\r\n    return {\r\n      open: false,\r\n      labelCol: {\r\n        span: 6,\r\n      },\r\n      wrapperCol: {\r\n        span: 14,\r\n      },\r\n      id: undefined,\r\n      chuyenmuc: {\r\n        giaTri: '',\r\n        maThongTin:'',\r\n        thuTuHienThi: '',\r\n        ghiChu: '',  \r\n        maDanhMuc:'03'   \r\n      },\r\n      rules:{\r\n        maThongTin:{\r\n          required: true, message: 'Mã chuyên mục không được để trống', trigger: 'change'\r\n        },\r\n        giaTri:{\r\n          required: true, message: 'Tên chuyên mục không được để trống', trigger: 'change'\r\n        },\r\n        thuTuHienThi:[\r\n          {\r\n            required: true, message: 'Thứ tự không được để trống', trigger: 'change'\r\n          },\r\n          {\r\n            type: 'integer', message: 'Thứ tự phải là kiểu số', trigger: 'blur'\r\n          }\r\n          \r\n        ],\r\n        \r\n      },\r\n      \r\n    };\r\n  },\r\n  methods:{\r\n    async showModal(id) {\r\n      this.open = true\r\n      if(id) {\r\n        var res = await APIService.get(`/ThongTinDanhMuc/${id}`)\r\n        this.id = id\r\n        this.chuyenmuc = res.data.data\r\n      }\r\n    },\r\n    closeModal() {\r\n      this.open = false\r\n      this.id = undefined\r\n      this.resetForm()\r\n    },\r\n    \r\n    handleOk() {\r\n      this.$refs.formRef.validate().then(async () => {\r\n        var duplicateCategory = false\r\n        var duplicateOrder = false\r\n        if (this.id) {\r\n          await APIService.put(`/ThongTinDanhMuc/edit/${this.id}`,this.chuyenmuc) \r\n          duplicateCategory = response.data.message == \"Mã bị trùng\" ? true : false;\r\n          duplicateOrder = response.data.message == \"Thứ tự hiển thị bị trùng\" ? true : false;\r\n        }\r\n        else {\r\n          await APIService.post(\"ThongTinDanhMuc/create\",this.chuyenmuc)\r\n          duplicateCategory = response.data.message == \"Mã bị trùng\" ? true : false;\r\n          duplicateOrder = response.data.message == \"Thứ tự hiển thị bị trùng\" ? true : false;\r\n          console.log('chhuydsen muc', this.chuyenmuc)\r\n        }\r\n        if (duplicateCategory == false && duplicateOrder == false) {\r\n          \r\n          this.closeModal()\r\n          this.$message.success(\"Thành công\")  \r\n          this.$emit('addSuccess')\r\n         \r\n        } else if (duplicateCategory) {\r\n          this.isLoading = false\r\n          this.$message.error(\"Trùng mã danh mục\")\r\n        } else {\r\n          this.isLoading = false\r\n          this.$message.error(\"Trùng thứ tự hiển thị\")\r\n        }\r\n        this.closeModal()\r\n        this.$emit('addSuccess')\r\n      }).catch(error => {\r\n        console.log('error', error);\r\n      });\r\n    },\r\n    resetForm() {\r\n      this.$refs.formRef.resetFields();\r\n    },\r\n  }\r\n};\r\n</script>\r\n\r\n<style>\r\n</style>"],"mappings":"AA8BA,OAAOA,UAAS,MAAO,sBAAsB;AAC7C,SAASC,OAAM,QAAS,uBAAuB;AAC/C,eAAe;EACbC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,IAAI,EAAE,KAAK;MACXC,QAAQ,EAAE;QACRC,IAAI,EAAE;MACR,CAAC;MACDC,UAAU,EAAE;QACVD,IAAI,EAAE;MACR,CAAC;MACDE,EAAE,EAAEC,SAAS;MACbC,SAAS,EAAE;QACTC,MAAM,EAAE,EAAE;QACVC,UAAU,EAAC,EAAE;QACbC,YAA