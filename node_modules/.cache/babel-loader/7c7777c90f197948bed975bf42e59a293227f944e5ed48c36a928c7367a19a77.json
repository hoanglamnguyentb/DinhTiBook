   </b-card-header>\r\n          <b-card-body class=\"border-bottom-dashed border-bottom\">\r\n            <b-form>\r\n              <b-row class=\"g-3\">\r\n                <b-col xl=\"6\">\r\n                  <div class=\"search-box\">\r\n                    <input type=\"text\" class=\"form-control search\"\r\n                      placeholder=\"Tiêu đề\" v-model=\"formSearch.nameTextFilter\" />\r\n                    <i class=\"ri-search-line search-icon\"></i>\r\n                  </div>\r\n                </b-col>\r\n                <b-col xl=\"6\">\r\n                  <b-row class=\"g-3\">\r\n                    <b-col sm=\"4\">\r\n                      <div class=\"\">\r\n                        <flat-pickr v-model=\"formSearch.publicDateFilter\" placeholder=\"Ngày hiển thị\" :config=\"rangeDateconfig\"\r\n                          class=\"form-control\"></flat-pickr>\r\n                      </div>\r\n                    </b-col>\r\n                    <b-col sm=\"4\">\r\n                      <div>\r\n                        <Multiselect class=\"form-control\" v-model=\"formSearch.isPublicFilter\" :close-on-select=\"true\" :searchable=\"true\"\r\n                          :create-option=\"true\" :options=\"[\r\n                      { value: '', label: 'Trạng thái', disabled: true },\r\n                      { value: '', label: '-Chọn-' },\r\n                      { value: '1', label: 'Xuất bản' },\r\n                      { value: '0', label: 'Bản thảo' },\r\n    ]\" />\r\n                      </div>\r\n                    </b-col>\r\n\r\n                    <b-col sm=\"4\">\r\n                      <div>\r\n                        <b-button type=\"button\" variant=\"primary\" class=\"w-100\" @click=\"SearchData\">\r\n                          <i class=\"ri-equalizer-fill me-2 align-bottom\"></i>Tìm kiếm\r\n                        </b-button>\r\n                      </div>\r\n                    </b-col>\r\n                  </b-row>\r\n                </b-col>\r\n              </b-row>\r\n            </b-form>\r\n          </b-card-body>\r\n          <b-card-body>\r\n            <div>\r\n              <div class=\"table-responsive table-card mb-1\">\r\n                <table v-if=\"lstBaiViet.length > 0\" class=\"table align-middle\" id=\"customerTable\">\r\n                  <thead class=\"table-light text-muted\">\r\n                    <tr>\r\n                      <th scope=\"col\" style=\"width: 50px\">\r\n                        <div class=\"form-check\">\r\n                          <input class=\"form-check-input\" type=\"checkbox\" id=\"checkAll\" value=\"option\" />\r\n                        </div>\r\n                      </th>\r\n\r\n                      <th class=\"sort\" data-sort=\"id\"></th>\r\n                      <th class=\"sort\" data-sort=\"\">Tác vụ</th>\r\n                      <th class=\"sort\" data-sort=\"nameText\">Tên văn bản</th>\r\n                      <th class=\"sort\" data-sort=\"number\">Số hiệu</th>\r\n                      <th class=\"sort\" data-sort=\"effectiveDate\">Ngày có hiệu lực</th>\r\n                      <th class=\"sort\" data-sort=\"agencyIssued\">Cơ quan ban hành</th>\r\n                      <th class=\"sort\" data-sort=\"publicDate\">Ngày ban hành</th>\r\n                      <th class=\"sort\" data-sort=\"status\">Trạng thái</th>\r\n                      <th class=\"sort\" data-sort=\"documentType\">Loại văn bản</th>\r\n                      <th class=\"sort\" data-sort=\"document  \">Tài liệu</th>\r\n                      <th></th>\r\n                    </tr>\r\n                  </thead>\r\n                  <tbody class=\"list form-check-all\">\r\n                    <tr v-for=\"(item, index) of lstBaiViet\" :key=\"index\">\r\n                      <th scope=\"row\">\r\n                        <div class=\"form-check\">\r\n                          <input class=\"form-check-input\" type=\"checkbox\" name=\"chk_child\" value=\"option1\" />\r\n                        </div>\r\n                      </th>\r\n                      <td class=\"id\" style=\"display: none\">\r\n                        <b-link href=\"javascript:void(0);\" class=\"fw-medium link-primary\">{{ item.customerId }}</b-link>\r\n                      </td>\r\n                      <td></td>\r\n                      <td class=\"tacvu\">\r\n                        <li style=\"list-style: none;\">\r\n                        <b-dropdown id=\"dropdown-1\" text=\"Thao tác\" class=\"m-md-2\" variant=\"outline-primary\">\r\n                          <b-dropdown-item>\r\n                            <b-link\r\n                              variant=\"text-info\"\r\n                              class=\"d-inline-block edit-item-btn\"\r\n                              @click=\"openModalDetail(record.id)\"\r\n                            >\r\n                              <i class=\"ri-information-line fs-14\"></i> Xem chi tiết\r\n                            </b-link>\r\n                          </b-dropdown-item>\r\n                          <b-dropdown-item>\r\n                            <b-link\r\n                                variant=\"text-primary\"\r\n                                class=\"d-inline-block edit-item-btn\"\r\n                                @click=\"openModalEdit(record.id)\"\r\n                              >\r\n                                <i class=\"ri-pencil-fill fs-16\"></i> Chỉnh sửa\r\n                              </b-link>\r\n                          </b-dropdown-item>\r\n                          <b-dropdown-item>\r\n                            <b-link\r\n                                class=\"text-danger d-inline-block remove-item-btn\"\r\n                                @click=\"deleteXa(record.id)\"\r\n                              >\r\n                                <i class=\"ri-delete-bin-5-fill fs-16\"></i> Xóa\r\n                              </b-link>\r\n                          </b-dropdown-item>\r\n                        </b-dropdown>\r\n                      </li>\r\n                      </td>\r\n                      <td class=\"ten\">{{ item.nameText }}</td>\r\n                      <td class=\"sohieu\">{{ item.number }}</td>\r\n                      <td class=\"ngayhieuluc\">{{ formatDate(item.effectiveDate)}}</td>\r\n                      <td class=\"coquanbanhanh\">{{  item.agencyIssued }}</td>\r\n                      <td class=\"ngaybanhanh\">{{ formatDate(item.publicDate) }}</td>\r\n                      <td class=\"trangthai\">{{ item.status }}</td>\r\n                      <td class=\"loaivanban\">{{ item.documentType }}</td>\r\n                      <td class=\"tailieu\">{{ item.document }}</td>\r\n\r\n                    </tr>\r\n                  </tbody>\r\n                </table>\r\n                <div v-if=\"lstBaiViet.length === 0\" class=\"noresult\">\r\n                  <div class=\"text-center\">\r\n                    <lottie class=\"avatar-xl\" colors=\"primary:#121331,secondary:#08a88a\" :options=\"defaultOptions\"\r\n                      :height=\"75\" :width=\"75\" />\r\n                    <h5 class=\"mt-2\">Không tìm thấy dữ liệu</h5>\r\n                    <p class=\"text-muted mb-0\">\r\n                      Hiện tại chưa có dữ liệu để hiển thị. Vui lòng thêm mới hoặc Import.\r\n                    </p>\r\n                  </div>\r\n                </div>\r\n              </div>\r\n\r\n              <Pagination v-if=\"optionPage.totalCount > 20\" :total=\"optionPage.totalCount\"\r\n                :current=\"optionPage.pageIndex\" :per-page=\"optionPage.pageSize\" :pageRange=\"optionPage.totalCount\"\r\n                @page-changed=\"handlePageChange\" />\r\n            </div>\r\n          </b-card-body>\r\n        </b-card>\r\n    </b-col>\r\n  </b-row>\r\n</template>\r\n\r\n<script>\r\nimport Multiselect from \"@vueform/multiselect\";\r\nimport \"@vueform/multiselect/themes/default.css\";\r\nimport flatPickr from \"vue-flatpickr-component\";\r\nimport \"flatpickr/dist/flatpickr.css\";\r\n\r\nimport PageHeader from \"@/components/page-header\";\r\nimport appConfig from \"../../../../app.config\";\r\nimport Swal from \"sweetalert2\";\r\nimport Lottie from \"@/components/widgets/lottie.vue\";\r\nimport animationData from \"@/components/Animation/msoeawqm.json\";\r\nimport animationData1 from \"@/components/Animation/gsqxdxog.json\";\r\nimport Pagination from \"../../../components/widgets/Pagination.vue\";\r\nimport APIService from \"@/helpers/ApiService\"\r\n\r\nimport { toast } from 'vue3-toastify';\r\nimport 'vue3-toastify/dist/index.css';\r\n\r\nimport Common from \"@/helpers/Common\"\r\n\r\n\r\nexport default {\r\n  page: {\r\n    title: \"Quản lý văn bản \",\r\n    meta: [{\r\n      name: \"Quản lý văn bản\",\r\n      content: appConfig.description,\r\n    },],\r\n  },\r\n  data() {\r\n    return {\r\n      title: \"Quản lý văn bản\",\r\n      items: [{\r\n        text: \"Dashboard\",\r\n        href: \"/\",\r\n      },\r\n      {\r\n        text: \"Quản lý văn bản \",\r\n        active: true,\r\n      },\r\n      ],\r\n      date1: null,\r\n      date: null,\r\n      addCustomerModal: false,\r\n      editCustomerModal: false,\r\n      tableColumns: [\r\n        { title: \"Tiêu đề\", dataIndex: \"title\" },\r\n        { title: \"Chuyên mục\", dataIndex: \"loai\" },\r\n        { title: \"Trạng thái\", dataIndex: \"isPublic\" },\r\n        { title: \"Ngày hiển thị\", dataIndex: \"publicDate\", align:'center' },\r\n        { title: \"Ngày tạo\", dataIndex: \"createdDate\" },\r\n        { title: \"\", dataIndex: \"action\" },\r\n      ],\r\n      rangeDateconfig: {\r\n        wrap: true, // set wrap to true only when using 'input-group'\r\n        altFormat: \"M j, Y\",\r\n        altInput: true,\r\n        dateFormat: \"d M, Y\",\r\n        mode: \"range\",\r\n      },\r\n      config: {\r\n        enableTime: false,\r\n        dateFormat: \"d M, Y\",\r\n      },\r\n      searchQuery: null